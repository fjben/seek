        <http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id2137instance> ;
    ns1:booktitle "Proceeding of the Workshop on Semantics for Business Process Management at the 3rd European Semantic Web Conference 2006"^^xsd:string ;
    ns1:month "June"^^xsd:string ;
    ns1:pages "1-12"^^xsd:string ;
    ns1:title "Automatic User Support for Business Process Modeling"^^xsd:string ;
    ns1:year "2006"^^xsd:string .

