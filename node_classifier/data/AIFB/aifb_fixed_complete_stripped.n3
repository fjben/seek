    ns1:booktitle "Proceedings of the International Conference on Intelligent User Interfaces"^^xsd:string ;
    ns1:hasProject <http://www.aifb.uni-karlsruhe.de/Projekte/viewProjektOWL/id50instance>,
        <http://www.aifb.uni-karlsruhe.de/Projekte/viewProjektOWL/id74instance> ;
    ns1:title "Porting Natural Language Interfaces between Domains -- An Experimental User Study with the ORAKEL Syste"^^xsd:string ;
    ns1:year "2007"^^xsd:string .

