    ns1:pages "488-497"^^xsd:string ;
    ns1:series "Lecture Notes in Computer Science"^^xsd:string ;
    ns1:title "Mechanisms for Structuring Knowledge-Based Systems"^^xsd:string ;
    ns1:volume "856"^^xsd:string ;
    ns1:year "1994"^^xsd:string .

