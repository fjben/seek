    ns1:isAbout <http://www.aifb.uni-karlsruhe.de/Forschungsgebiete/viewForschungsgebietOWL/id12instance> ;
    ns1:month "August"^^xsd:string ;
    ns1:pages "987-991"^^xsd:string ;
    ns1:title "A Petri-net based Approach to Performance Management of collaborative Business Processes"^^xsd:string ;
    ns1:year "2005"^^xsd:string .

