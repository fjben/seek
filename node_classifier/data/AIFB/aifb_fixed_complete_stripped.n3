    ns1:isAbout <http://www.aifb.uni-karlsruhe.de/Forschungsgebiete/viewForschungsgebietOWL/id55instance>,
        <http://www.aifb.uni-karlsruhe.de/Forschungsgebiete/viewForschungsgebietOWL/id84instance> ;
    ns1:month "August"^^xsd:string ;
    ns1:pages "500--506"^^xsd:string ;
    ns1:title "On the Influence of Lifetime Learning on Selection Pressure"^^xsd:string ;
