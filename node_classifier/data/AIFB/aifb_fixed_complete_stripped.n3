    ns1:booktitle "Proceedings of the 2nd Workshop on OWL: Experiences and Directions"^^xsd:string ;
    ns1:hasProject <http://www.aifb.uni-karlsruhe.de/Projekte/viewProjektOWL/id42instance>,
