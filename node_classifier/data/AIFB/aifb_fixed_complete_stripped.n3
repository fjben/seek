    ns1:month "November"^^xsd:string ;
    ns1:number "2"^^xsd:string ;
    ns1:pages "148-169"^^xsd:string ;
    ns1:title "Towards Self-Organizing Knowledge Intensive Processes"^^xsd:string ;
    ns1:volume "0"^^xsd:string ;
    ns1:year "2005"^^xsd:string .

