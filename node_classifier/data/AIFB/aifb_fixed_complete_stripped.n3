        <http://www.aifb.uni-karlsruhe.de/Projekte/viewProjektOWL/id49instance>,
        <http://www.aifb.uni-karlsruhe.de/Projekte/viewProjektOWL/id50instance> ;
    ns1:isAbout <http://www.aifb.uni-karlsruhe.de/Forschungsgebiete/viewForschungsgebietOWL/id131instance>,
        <http://www.aifb.uni-karlsruhe.de/Forschungsgebiete/viewForschungsgebietOWL/id55instance> ;
    ns1:month "November"^^xsd:string ;
