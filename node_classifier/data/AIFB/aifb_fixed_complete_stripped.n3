    ns1:booktitle "Proceedings of the International Workshop Intelligent Information Integration during the 21st German Annual Conference on Artificial Intelligence (KI'97), Freiburg, Germany, September 9-12, 1997"^^xsd:string ;
    ns1:month "September"^^xsd:string ;
    ns1:title "Semantically Enriched Subnets of the WWW"^^xsd:string ;
    ns1:year "1997"^^xsd:string .

