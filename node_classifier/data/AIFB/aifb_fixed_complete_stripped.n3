    ns1:journal "SIGKDD Exploration Special Issue on Scalable Algorithms"^^xsd:string ;
    ns1:pages "71-80"^^xsd:string ;
    ns1:title "Mining Frequent Patterns with Counting Inference"^^xsd:string ;
    ns1:year "2000"^^xsd:string .

