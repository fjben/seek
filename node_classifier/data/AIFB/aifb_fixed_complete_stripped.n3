    ns1:isAbout <http://www.aifb.uni-karlsruhe.de/Forschungsgebiete/viewForschungsgebietOWL/id98instance> ;
    ns1:title "Facilitate Successful Cross-Organisational Business Processes - A Business Case"^^xsd:string ;
    ns1:year "2005"^^xsd:string .

