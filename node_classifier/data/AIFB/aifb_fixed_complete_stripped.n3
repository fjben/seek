    ns1:hasProject <http://www.aifb.uni-karlsruhe.de/Projekte/viewProjektOWL/id9instance> ;
    ns1:month "November"^^xsd:string ;
    ns1:title "A Unifying View on Business Process Modelling and Knowledge Engineering"^^xsd:string ;
    ns1:year "1996"^^xsd:string .

<http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id60instance> a ns1:InProceedings,
        ns1:Publication ;
    ns1:abstract "Current courseware is often tightly coupled with a specific e-learning system for its access and presentation. We identify the system components and their interaction in creating, publishing, accessing and executing platform independent learning content. We present a system model aimed at using inexpensive of-the-shelf components to supply the core functionality while being ready to integrate flexible and powerful content management and e-learning support systems in future."^^xsd:string ;
